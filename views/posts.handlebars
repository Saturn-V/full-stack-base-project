<div class="row">
  <div class="col-md-4">
    <h1>Posts</h1>
  </div>
</div>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <dl class="dl-horizontal">
      {{#each posts}}
      <dt>{{creator}}</dt>

      <dt>Title</dt>
      <dd>{{title}}</dd>
      <dt>Body</dt>
      <dd>{{body}}</dd>
      {{/each}}
    </dl>
  </div>
</div>
